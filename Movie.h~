#include <iostream>
#include <string.h>
using namespace std;

class Movie
{
 public:
  Movie(string*);
  string* getDescription();
  int getID();
  char getTitle();
  char getDirector();
  int getYear();
  float getDuration();
  float getRating();
  int getType();
 private:
  string* description;
  float rating;
  char director[80];
  float duration;
}
